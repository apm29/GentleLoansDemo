<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <gradient android:startColor="@color/gray_not_enabled" android:endColor="@color/white_foreground" android:angle="-90" android:centerColor="@color/white_foreground" android:centerX="0.99"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <gradient android:startColor="@color/white_foreground" android:endColor="@color/white_foreground" android:angle="-90" android:centerColor="@color/white_foreground" android:centerX="0.99"/>
        </shape>
    </item>
</selector>
